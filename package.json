{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@geist-ui/react": "^2.2.0",
    "@geist-ui/react-icons": "^1.0.1",
    "@mui/material": "^5.0.6",
    "@umijs/hooks": "^1.9.3",
    "@umijs/plugin-esbuild": "^1.4.1",
    "browser-fs-access": "^0.21.0",
    "browserslist": "^4.17.6",
    "caniuse-lite": "^1.0.30001276",
    "dayjs": "^1.10.7",
    "dd_server_api_web": "^1.4.2",
    "github-markdown-css": "^4.0.0",
    "markdown-it": "^12.2.0",
    "markdown-navbar": "^1.4.3",
    "qrcode.react": "^1.0.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-markdown": "^7.1.0",
    "react-markdown-editor-lite": "^1.3.1",
    "react-syntax-highlighter": "^15.4.4",
    "remark-gfm": "^2.0.0",
    "umi": "^3.5.20",
    "umi-plugin-keep-alive": "^0.0.1-beta.29"
  },
  "devDependencies": {
    "@types/markdown-navbar": "^1.4.0",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@umijs/preset-react": "~1.8.25",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "yorkie": "^2.0.0"
  }
}
